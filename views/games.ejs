<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Games</title>
</head>
<body>
  <% games.forEach((game) => { %>
    <div>
      <a href="/games/<%= game.game_id %>"> <%= game.name %> </a>
    </div>
  <% }) %>
</body>
</html>